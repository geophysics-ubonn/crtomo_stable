c COMMON

c Anzahl der vorgegebenen Potentialwerte (Dirichletsche Randbedingung)
        integer         * 4     rwdanz

c Knotennummern der vorgegebenen Potentialwerte
        integer         * 4     rwdnr(rwdmax)

c Randwerte (Dirichletsche Randbedingung)
        real            * 8     rwddc(rwdmax)
        complex         * 16    rwd(rwdmax)

c Zeiger ab wann Potentialwerte automatisch belegt werden sollen
